<div ng-repeat="(view_key, view_value) in totalHeaders">
  <div class="col-xs-12">
    <strong>{{ view_value.label }}</strong>
  </div>
  <div class="col-xs-6">
    <div class="col-xs-12 compact">
      Included
    </div>
    <div class="col-xs-12 compact">
      <div class="well drop-zone"
           data-drop="true"
           ng-model="view_value.selectedHeaders"
           data-jqyoui-options="{ accept:'.exclude-item' }"
           jqyoui-droppable="droppableOptions"
           style="min-height: 100px; width: 100%">
        <div class="btn btn-success include-item"
             ng-repeat="selected in view_value.selectedHeaders"
             data-drag="true"
             data-jqyoui-options="{revert: 'invalid'}"
             ng-model="selected"
             ng-if="view_value.selectedHeaders.indexOf(selected) != -1"
             jqyoui-draggable>{{ selected.label }}</div>
      </div>
    </div>
  </div>
  <div class="col-xs-6">
    <div class="col-xs-12 compact">
      Excluded
    </div>
    <div class="col-xs-12 compact">
      <div class="well drop-zone"
           data-drop="true"
           ng-model="view_value.availableHeaders"
           data-jqyoui-options="{ accept:'.include-item'}"
           jqyoui-droppable="droppableOptions"
           style="min-height: 100px; width: 100%">
        <div class="btn btn-danger exclude-item"
             ng-repeat="available in view_value.availableHeaders"
             data-drag="true"
             data-jqyoui-options="{revert: 'invalid'}"
             ng-model="available"
             ng-if="view_value.availableHeaders.indexOf(available) != -1"
             jqyoui-draggable>{{ available.label }}</div>
      </div>
    </div>
  </div>
</div>
