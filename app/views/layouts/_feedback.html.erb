<script type="text/javascript">
  function browserIsIE() {
    return navigator.userAgent.indexOf("Trident") >= 0;
  }
  function browserNewerThanIE9() {
    return /Trident\/([^;]+)/.test(navigator.userAgent) && parseFloat(RegExp["$1"]) > 5;
  }
  function popBugReporter() {
    if ($('#defects_index').length) {
      $('.bug-report-element').first().click();
    }
  }

  window.ATL_JQ_PAGE_PROPS = $.extend(window.ATL_JQ_PAGE_PROPS, {
    '9a6028f0': {
      triggerFunction: function(showIssueCollector) {
        $('.bug-report-element').click(function(e) {
          e.preventDefault();
          showIssueCollector();
        })

        if (browserIsIE() && browserNewerThanIE9()) {
          setTimeout(function() {
            popBugReporter();
          }, 500);
        } else {
          $(window).load(function() {
            popBugReporter();
          });
        }
      },
      fieldValues: {
        customfield_11103: '<%= j current_user.try(:email)%>'
      }
    },
    '52db04e5': {
      fieldValues: {
        email: '<%= j current_user.try(:email)%>'
      }
    }
  });
